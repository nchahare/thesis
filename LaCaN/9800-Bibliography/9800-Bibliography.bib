@article{collinet2021,
	abstract = {How the shape of embryos and organs emerges during development is a fundamental question that has fascinated scientists for centuries. Tissue dynamics arise from a small set of cell behaviours, including shape changes, cell contact remodelling, cell migration, cell division and cell extrusion. These behaviours require control over cell mechanics, namely active stresses associated with protrusive, contractile and adhesive forces, and hydrostatic pressure, as well as material properties of cells that dictate how cells respond to active stresses. In this Review, we address how cell mechanics and the associated cell behaviours are robustly organized in space and time during tissue morphogenesis. We first outline how not only gene expression and the resulting biochemical cues, but also mechanics and geometry act as sources of morphogenetic information to ultimately define the time and length scales of the cell behaviours driving morphogenesis. Next, we present two idealized modes of how this information flows \textemdash{} how it is read out and translated into a biological effect \textemdash{} during morphogenesis. The first, akin to a programme, follows deterministic rules and is hierarchical. The second follows the principles of self-organization, which rests on statistical rules characterizing the system's composition and configuration, local interactions and feedback. We discuss the contribution of these two modes to the mechanisms of four very general classes of tissue deformation, namely tissue folding and invagination, tissue flow and extension, tissue hollowing and, finally, tissue branching. Overall, we suggest a conceptual framework for understanding morphogenetic information that encapsulates genetics and biochemistry as well as mechanics and geometry as information modules, and the interplay of deterministic and self-organized mechanisms of their deployment, thereby diverging considerably from the traditional notion that shape is fully encoded and determined by genes.},
	author = {Collinet, Claudio and Lecuit, Thomas},
	copyright = {2021 Springer Nature Limited},
	doi = {10.1038/s41580-020-00318-6},
	file = {C\:\\Users\\NChahare\\Zotero\\storage\\GFQ4TWXK\\Collinet and Lecuit - 2021 - Programmed and self-organized flow of information .pdf},
	issn = {1471-0080},
	journal = {Nature Reviews Molecular Cell Biology},
	keywords = {âœ”ï¸,ðŸ“š,Skimmed},
	langid = {english},
