@article{khalilgharibi2019,
  title = {Stress Relaxation in Epithelial Monolayers Is Controlled by the Actomyosin Cortex},
  author = {Khalilgharibi, Nargess and Fouchard, Jonathan and Asadipour, Nina and Barrientos, Ricardo and Duda, Maria and Bonfanti, Alessandra and Yonis, Amina and Harris, Andrew and Mosaffa, Payman and Fujita, Yasuyuki and Kabla, Alexandre and Mao, Yanlan and Baum, Buzz and Mu{\~n}oz, Jos{\'e} J. and Miodownik, Mark and Charras, Guillaume},
