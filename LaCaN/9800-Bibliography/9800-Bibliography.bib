	month = may,
	number = {18},
	pages = {5726--5731},
	publisher = {{Proceedings of the National Academy of Sciences}},
	title = {Emergence of Homeostatic Epithelial Packing and Stress Dissipation through Divisions Oriented along the Long Cell Axis},
	urldate = {2023-01-11},
	volume = {112},
