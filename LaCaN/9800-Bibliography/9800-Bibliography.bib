	issn = {1742-7061},
	journal = {Acta Biomaterialia},
	keywords = {Skimmed},
	langid = {english},
	month = oct,
	pages = {32--42},
	shorttitle = {Deciphering and Engineering Tissue Folding},
	title = {Deciphering and Engineering Tissue Folding: {{A}} Mechanical Perspective},
	urldate = {2023-01-18},
