@article{ambrosi2017,
  title = {Mechanobiology and Morphogenesis in Living Matter: A Survey},
  shorttitle = {Mechanobiology and Morphogenesis in Living Matter},
  author = {Ambrosi, D. and Beloussov, L. V. and Ciarletta, P.},
