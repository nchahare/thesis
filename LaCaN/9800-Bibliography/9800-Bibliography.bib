	journal = {Journal of The Royal Society Interface},
	langid = {english},
	month = aug,
	number = {157},
	pages = {20190233},
	shorttitle = {Growth and Remodelling of Living Tissues},
	title = {Growth and Remodelling of Living Tissues: Perspectives, Challenges and Opportunities},
