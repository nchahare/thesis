@article{armon2021,
  title = {Modeling Epithelial Tissues as Active-Elastic Sheets Reproduce Contraction Pulses and Predict Rip Resistance},
  author = {Armon, Shahaf and Bull, Matthew S. and Moriel, Avraham and Aharoni, Hillel and Prakash, Manu},
