	month = dec,
	number = {51},
	pages = {20820--20825},
	publisher = {{Proceedings of the National Academy of Sciences}},
	title = {F-Actin Buckling Coordinates Contractility and Severing in a Biomimetic Actomyosin Cortex},
	urldate = {2023-03-09},
	volume = {109},
	year = {2012},
	bdsk-url-1 = {https://doi.org/10.1073/pnas.1214753109}}

