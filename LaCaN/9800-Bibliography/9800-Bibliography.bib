@phdthesis{chahare2016,
  title = {Design and Fabrication of Miniature Shear Device for Cell Mechanics},
  author = {Chahare, Nimesh R.},
  year = {2016},
  month = jun,
  abstract = {Cells are active soft matter and their mechanical properties are primarily dictated through cytoskeletal composite network comprised of linear and branched polymers which can change dynamically, cell cytoskeleton is highly complex and dynamical in nature, such that cell can change its internal struc- ture in response to external stimuli of mechnical or biochemical nature by polymerisation and depolymerisation of filaments. This process of sensing external stimuli is called mechanosensitivity. Physical forces regulate physiological processes in human body, and dysfunctional processes may result in diseases. The mechanosensing of cells can only be understood by systematic experiments, and these experiments need sophisticated laboratory appara- tuses for mechanical stimulation of cells. In this work, we construct a custom miniature fluid shear device which may be used to apply shear stresses ranging from (0.5 to 8 Pa) through a cone plate rheometric method. This device is compact and light to be mounted on inverted microscope for visualizing cell cytoskeleton deformations live. We are in process of securing Indian patent, and due to patent disclosure we only report ideation and calibration of device. This device can be used for studying cell mechanobiology and rheology of other soft matter.},
  copyright = {All rights reserved},
  school = {IISc Bangalore},
  keywords = {Unread}
}

@article{chan2018,
