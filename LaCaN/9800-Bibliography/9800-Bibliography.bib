@phdthesis{chahare2016,
	abstract = {Cells are active soft matter and their mechanical properties are primarily dictated through cytoskeletal composite network comprised of linear and branched polymers which can change dynamically, cell cytoskeleton is highly complex and dynamical in nature, such that cell can change its internal struc- ture in response to external stimuli of mechnical or biochemical nature by polymerisation and depolymerisation of filaments. This process of sensing external stimuli is called mechanosensitivity. Physical forces regulate physiological processes in human body, and dysfunctional processes may result in diseases. The mechanosensing of cells can only be understood by systematic experiments, and these experiments need sophisticated laboratory appara- tuses for mechanical stimulation of cells. In this work, we construct a custom miniature fluid shear device which may be used to apply shear stresses ranging from (0.5 to 8 Pa) through a cone plate rheometric method. This device is compact and light to be mounted on inverted microscope for visualizing cell cytoskeleton deformations live. We are in process of securing Indian patent, and due to patent disclosure we only report ideation and calibration of device. This device can be used for studying cell mechanobiology and rheology of other soft matter.},
	author = {Chahare, Nimesh R.},
	copyright = {All rights reserved},
	keywords = {Unread},
	month = jun,
	school = {IISc Bangalore},
	title = {Design and Fabrication of Miniature Shear Device for Cell Mechanics},
	year = {2016}}

@article{chan2018,
	abstract = {The surfaces of many hollow or tubular tissues/organs in our respiratory, gastrointestinal, and urogenital tracts are covered by mucosa with folded patterns. The patterns are induced by mechanical instability of the mucosa under compression due to constrained growth. Recapitulating this folding process in vitro will facilitate the understanding and engineering of mucosa in various tissues/organs. However, scant attention has been paid to address the challenge of reproducing mucosal folding. Here we mimic the mucosal folding process using a cell-laden hydrogel film attached to a prestretched tough-hydrogel substrate. The cell-laden hydrogel constitutes a human epithelial cell lining on stromal component to recapitulate the physiological feature of a mucosa. Relaxation of the prestretched tough-hydrogel substrate applies compressive strains on the cell-laden hydrogel film, which undergoes mechanical instability and evolves into morphological patterns. We predict the conditions for mucosal folding as well as the morphology of and strain in the folded artificial mucosa using a combination of theory and simulation. The work not only provides a simple method to fold artificial mucosa but also demonstrates a paradigm in tissue engineering via harnessing mechanical instabilities guided by quantitative mechanics models.},
	author = {Chan, Hon Fai and Zhao, Ruike and Parada, German A. and Meng, Hu and Leong, Kam W. and Griffith, Linda G. and Zhao, Xuanhe},
	doi = {10.1073/pnas.1802361115},
