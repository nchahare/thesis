@article{stanton2019,
  title = {Extracellular Matrix Type Modulates Mechanotransduction of Stem Cells},
  author = {Stanton, Alice E. and Tong, Xinming and Yang, Fan},
  year = {2019},
  month = sep,
  journal = {Acta Biomaterialia},
  volume = {96},
